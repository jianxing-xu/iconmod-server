{
	"name": "@iconify/api",
	"description": "Iconify API",
	"author": "Vjacheslav Trushkin",
	"license": "MIT",
	"version": "3.1.0",
	"type": "module",
	"bugs": "https://github.com/iconify/api/issues",
	"homepage": "https://github.com/iconify/api",
	"repository": {
		"type": "git",
		"url": "https://github.com/iconify/api.git"
	},
	"packageManager": "yarn@1.22.22",
	"engines": {
		"node": ">=16.15.0"
	},
	"scripts": {
		"s": "yarn build && yarn start --watch",
		"dev": "yarn build:watch && yarn start:watch",
		"build": "tsc -b",
		"build:watch": "tsc -b",
		"test": "vitest --config vitest.config.mjs",
		"start": "node --expose-gc lib/index.js",
		"start:watch": "node --expose-gc --watch lib/index.js",
		"mgr": "npx prisma migrate dev",
		"docker:build": "./docker.sh",
		"docker:start": "docker run -d -p 3000:3000 iconify/api",
		"docker:stop": "docker ps -q --filter ancestor=iconify/api | xargs -r docker stop",
		"docker:cleanup": "docker ps -q -a --filter ancestor=iconify/api | xargs -r docker rm",
		"docker:publish": "docker push iconify/api"
	},
	"dependencies": {
		"@fastify/cookie": "^9.4.0",
		"@fastify/formbody": "^7.4.0",
		"@fastify/jwt": "^8.0.1",
		"@iconify/tools": "^4.0.4",
		"@iconify/types": "^2.0.0",
		"@iconify/utils": "^2.1.23",
		"@prisma/client": "5.19.0",
		"dotenv": "^16.4.5",
		"fastify": "^4.26.2",
		"prisma": "^5.19.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.7",
		"typescript": "^5.4.5",
		"vitest": "^1.5.2"
	}
}
